<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFX.EventsController">
    <children>
        <VBox alignment="TOP_CENTER">
            <children>
                <HBox prefHeight="52.0" prefWidth="600.0">
                    <children>
                        <Button fx:id="MainMenuPane" alignment="CENTER" focusTraversable="false" mnemonicParsing="false" onAction="#goToMainMenu" prefHeight="45.0" prefWidth="150.0" style="-fx-text-fill: #FFF; -fx-background-color: #313131; -fx-font-weight: BOLD;" text="Main Menu" textAlignment="CENTER">
                            <font>
                                <Font name="Segoe UI Symbol" size="20.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="ProfilePane" alignment="CENTER" focusTraversable="false" mnemonicParsing="false" onAction="#goToProfile" prefHeight="45.0" prefWidth="150.0" style="-fx-text-fill: #fff; -fx-background-color: #313131; -fx-font-weight: BOLD;" text="Profile" textAlignment="JUSTIFY">
                            <font>
                                <Font name="Segoe UI Symbol" size="20.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                            </HBox.margin>
                            <effect>
                                <DropShadow />
                            </effect>
                        </Button>
                        <Button fx:id="FriendsPane" alignment="CENTER" focusTraversable="false" mnemonicParsing="false" onAction="#goToFriends" prefHeight="45.0" prefWidth="150.0" style="-fx-text-fill: #fff; -fx-background-color: #313131; -fx-font-weight: BOLD;" text="Followed" textAlignment="JUSTIFY">
                            <font>
                                <Font name="Segoe UI Symbol" size="20.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                            </HBox.margin>
                            <effect>
                                <DropShadow />
                            </effect>
                        </Button>
                        <Button fx:id="EventsPane" alignment="CENTER" focusTraversable="false" mnemonicParsing="false" onAction="#goToEvents" prefHeight="45.0" prefWidth="150.0" style="-fx-text-fill: #fff; -fx-background-color: #414141; -fx-font-weight: BOLD;" text="Events" textAlignment="JUSTIFY">
                            <font>
                                <Font name="Segoe UI Symbol" size="20.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                            </HBox.margin>
                            <effect>
                                <DropShadow />
                            </effect>
                        </Button>
                        <Button fx:id="ChatPane" alignment="CENTER" focusTraversable="false" mnemonicParsing="false" onAction="#goToChat" prefHeight="45.0" prefWidth="150.0" style="-fx-text-fill: #fff; -fx-background-color: #313131; -fx-font-weight: BOLD;" text="Chat" textAlignment="JUSTIFY">
                            <font>
                                <Font name="Segoe UI Symbol" size="20.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                            </HBox.margin>
                            <effect>
                                <DropShadow />
                            </effect>
                        </Button>
                    </children>
                    <effect>
                        <DropShadow />
                    </effect>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
